{
  "name": "eonx-com/currencies",
  "description": "ISO 4217 compatible library for handling currencies",
  "keywords": [
    "eoneopay",
    "currencies",
    "formatter"
  ],
  "license": "BSD-3-Clause",
  "type": "library",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "eonx-com/standards": "^0.2",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.9",
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "3.*"
  },
  "replace": {
    "eoneopay/currencies": "*"
  },
  "autoload": {
    "psr-4": {
      "EoneoPay\\Currencies\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\EoneoPay\\Currencies\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
